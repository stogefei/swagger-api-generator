{
  "name": "api-swag-core",
  "version": "2.0.2-alpha.0",
  "description": "api生成工具",
  "license": "MIT",
  "main": "src/index.ts",
  "typings": "src/index.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stogefei/swagger-api-generator.git"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "build:doc": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "travis-deploy-once": "travis-deploy-once",
    "pub": "sh release.sh"
  },
  "devDependencies": {
    "@types/jasmine-ajax": "^3.3.0",
    "@types/jest": "^23.3.2",
    "@types/nprogress": "^0.2.0",
    "body-parser": "^1.19.0",
    "colors": "^1.3.2",
    "connect-multiparty": "^2.2.0",
    "cookie-parser": "^1.4.4",
    "coveralls": "^3.0.2",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.2.0",
    "jasmine-ajax": "^4.0.0",
    "jasmine-core": "^3.4.0",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "mockjs": "^1.1.0",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rollup": "^2.79.1",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.18.0",
    "semantic-release": "^15.9.16",
    "sh": "^0.0.3",
    "shelljs": "^0.8.3",
    "style-loader": "^1.0.0",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^24.0.2",
    "ts-loader": "^9.4.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-loader": "^3.5.4",
    "webpack": "^4.39.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "gitHead": "621db69065b8c058c8b2c219ff1a7f568ef4bb7d"
}
